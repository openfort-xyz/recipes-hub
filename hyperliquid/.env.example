# Copy this file to .env.local and fill in your Openfort credentials

# Openfort Project Keys (from dashboard: Project Settings > API Keys)
OPENFORT_PROJECT_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Openfort Shield Keys (from dashboard: Shield > API Keys)
OPENFORT_SHIELD_PUBLISHABLE_KEY=pk_test_your_shield_key
OPENFORT_SHIELD_ENCRYPTION_SHARE=your_shield_encryption_share

# If you want to use AUTOMATIC embedded wallet recovery, an encryption session is required.
# See: https://www.openfort.io/docs/products/embedded-wallet/react-native/quickstart/automatic
# For backend setup, check: https://github.com/openfort-xyz/openfort-backend-quickstart
OPENFORT_SHIELD_RECOVERY_BASE_URL=https://your-recovery-api.com

# Gas Sponsorship Policy
OPENFORT_ETHEREUM_PROVIDER_POLICY_ID=pol_your_policy_id

# Hyperliquid wallet address for balance fetching
HYPERLIQUID_WALLET_ADDRESS=0x_your_hyperliquid_wallet_address
